{
    "切换目录": "cd {/disk} {dir}",
    "文件列表": "dir",
    "ping": "ping {ip} {-n times}",
    "IP显示": "ipconfig",
    "清理DNS缓存": "ipconfig /flushdns",
    "启动程序进程": "start {exe}",
    "编译dll(MinGW64)": "g++ -shared -o {dll} {cpp} {args}",
    "编译exe(MinGW64)": "g++ -o {exe} {cpp} {args}",
    "运行py": "python {py} {args}"
}